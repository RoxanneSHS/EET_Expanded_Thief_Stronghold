IF
  Global("TSReveal0323","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobal("TSReveal0323","GLOBAL",1)
    SetAreaRestFlag(1)
    OpenDoor("Door01")
    OpenDoor("Door02")
    OpenDoor("Door03")
    Explore()
END

IF
  !GlobalTimerNotExpired("TSGuildDoors23","GLOBAL")
THEN
  RESPONSE #100
    SetGlobalTimer("TSGuildDoors23","GLOBAL",ONE_DAY)
    OpenDoor("Door01")
    OpenDoor("Door02")
    OpenDoor("Door03")
END


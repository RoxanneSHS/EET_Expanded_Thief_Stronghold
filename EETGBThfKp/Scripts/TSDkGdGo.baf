IF
  Global("TSPlot","GLOBAL",6)
  Global("TSDeactive","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("TSDeactive","LOCALS",1)
    Deactivate(Myself)
END

IF
  Global("TSDeactive","LOCALS",1)
  !Global("TSPlot","GLOBAL",6)
THEN
  RESPONSE #100
    SetGlobal("TSDeactive","LOCALS",0)
    Activate(Myself)
END

